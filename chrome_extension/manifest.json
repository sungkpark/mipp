{
    "manifest_version": 3,
    "name": "Mipp",
    "description": "Make It haPPen! Mippit!!",
    "version": "1.0",
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "action": {
        "default_icon": "images/mipp_extensions.png",
        "default_popup": "pages/popup/popup.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+P",
                "mac": "Command+P"
            }
        }
    }
}